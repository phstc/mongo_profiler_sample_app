<h1>Listing customers</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
    </tr>
  </thead>

  <tbody>
    <% @customers.each do |customer| %>
      <tr>
        <td><%= customer['first_name'] %></td>
        <td><%= customer['last_name'] %></td>
        <td><%= customer['email'] %></td>
      </tr>
    <% end %>
  </tbody>
</table>
